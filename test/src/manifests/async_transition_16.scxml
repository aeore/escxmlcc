<scxml initial="init" version="0.9" xmlns="http://www.w3.org/2005/07/scxml"><!--   node-size-and-position x=0 y=0 w=960 h=700  -->
 <datamodel>int value;</datamodel>
 <state id="A"><!--   node-size-and-position x=160 y=100 w=100 h=40  -->
  <transition cond="value &gt; 7" event="@e" target="B"><!--   edge-path [B]  pointx=0 pointy=10 offsetx=0 offsety=0  --></transition>
 </state>
 <state id="init"><!--   node-size-and-position x=170 y=30 w=80 h=30  -->
  <transition target="A"></transition>
 </state>
 <state id="B"><!--   node-size-and-position x=360 y=100 w=110 h=40  -->
  <transition target="Cluster"></transition>
 </state>
 <state id="Cluster" initial="C"><!--   node-size-and-position x=120 y=210 w=590 h=470  -->
  <state id="Cluster_2" initial="E"><!--   node-size-and-position x=140 y=200 w=430 h=260  -->
   <transition event="@z" target="G"><!--   edge-path [G]  pointx=0 pointy=10 offsetx=10 offsety=0  --></transition>
   <state id="E"><!--   node-size-and-position x=40 y=80 w=80 h=40  -->
    <transition target="F"></transition>
   </state>
   <state id="F"><!--   node-size-and-position x=220 y=80 w=80 h=40  -->
    <transition event="@x" target="H"><!--   edge-path [H]  x=400 y=410 x=390 y=420 pointx=0 pointy=-10 offsetx=-22 offsety=0  --></transition>
   </state>
  </state>
  <state id="C"><!--   node-size-and-position x=30 y=70 w=80 h=40  -->
   <transition event="t" target="D"><!--   edge-path [D]  pointx=0 pointy=10 offsetx=0 offsety=0  --></transition>
  </state>
  <state id="D"><!--   node-size-and-position x=310 y=70 w=90 h=40  -->
   <transition cond="value &lt; 4" event="@w" target="Cluster_2"></transition>
   <transition cond="value &gt; 4" event="@w" target="F"><!--   edge-path [F]  x=480 y=90 x=480 y=300  --></transition>
  </state>
  <state id="H"><!--   node-size-and-position x=30 y=400 w=80 h=40  -->
   <transition event="@" target="C"><!--   edge-path [C]  pointx=0 pointy=-20 offsetx=0 offsety=-5  --></transition>
  </state>
 </state>
 <state id="G"><!--   node-size-and-position x=850 y=520 w=80 h=40  --></state>
</scxml>